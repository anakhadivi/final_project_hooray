<div id='user_info'>
	<div id='welcome'>
		<h1>
			Welcome Blader!
		</h1>
	</div>
	<div id='users_show_links'>
		<nav>
			<ul>
				<li>
					<%=link_to 'Edit My Account', edit_user_path(current_user)%>
				</li>
				<li>
					<%=link_to 'View BladerSpots', posts_path%>
				</li>
				<li>
					<%=link_to 'Create New Spot', new_post_path%>
				</li>
				<li>
					<%=link_to 'Sign Out', destroy_user_session_path, method: :delete %>
				</li>
			</ul>
		</nav>
	</div>
</div>
<div id='user_spots'>
	<h2>Your Spots</h2>
	<%@user.posts.each do |post|%>
		<b>Location:</b>
		<br>
		<%=post.location%>
		<br>
		<%=link_to 'View Post', post_path(post)%>
		<br>
		<br>
	<%end%>
</div>

